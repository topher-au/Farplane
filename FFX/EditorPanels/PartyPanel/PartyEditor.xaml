<UserControl x:Class="Farplane.FFX.EditorPanels.PartyPanel.PartyEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:FFXEditor="clr-namespace:Farplane.FFX.EditorPanels"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:partyPanel="clr-namespace:Farplane.FFX.EditorPanels.PartyPanel"
             d:DesignHeight="400"
             d:DesignWidth="450"
             mc:Ignorable="d">
    <TabControl Margin="0" Padding="0">
        <TabItem Header="Stats">
            <partyPanel:PartyStats x:Name="PartyStats" />
        </TabItem>
        <TabItem Header="Abilities">
            <partyPanel:PartyAbilities x:Name="PartyAbilities" />
        </TabItem>
        <TabItem Header="Overdrive">
            <partyPanel:PartyOverdrive x:Name="PartyOverdrive" Margin="0,-2,0,0" />
        </TabItem>
    </TabControl>
</UserControl>
