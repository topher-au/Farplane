<UserControl x:Class="Farplane.FFX.EditorPanels.NotAvailablePanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="300"
             d:DesignWidth="300"
             mc:Ignorable="d">
    <GroupBox Name="GroupNYI" Header="Selection Not Available">
        <DockPanel VerticalAlignment="Center">
            <TextBlock Name="TextNYI"
                       TextAlignment="Center"
                       TextWrapping="Wrap">
                The selected option is not available in File Mode
            </TextBlock>
        </DockPanel>
    </GroupBox>
</UserControl>
