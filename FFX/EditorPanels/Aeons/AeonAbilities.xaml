<UserControl x:Class="Farplane.FFX.EditorPanels.Aeons.AeonAbilities"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Farplane.FFX.EditorPanels.Aeons"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             d:DesignHeight="400"
             d:DesignWidth="400"
             mc:Ignorable="d">
    <GroupBox Header="Abilities">
        <TabControl Name="TabAbilities">
            <TabItem Name="TabSkills" Header="Skills" />
            <TabItem Name="TabSpecial" Header="Special" />
            <TabItem Name="TabWhiteMagic" Header="White Magic" />
            <TabItem Name="TabBlackMagic" Header="Black Magic" />
        </TabControl>
    </GroupBox>
</UserControl>
